{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "rules": {
    "no-console": false,
    "interface-name": false,
    "variable-name": false,
    "object-literal-sort-keys": false,
    "max-classes-per-file": false,
    "trailing-comma": [
      true,
      {
        "multiline": {
          "objects": "always",
          "arrays": "always",
          "functions": "never"
        }
      }
    ]
  },
  "rulesDirectory": []
}
